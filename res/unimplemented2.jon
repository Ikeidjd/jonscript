mut int min = 1;
mut int max = 100;
mut int guess = 0;
mut bool win = false;

while(min <= max) {
    guess = (min + max) / 2;

    mut str input = "";
    while(input.byte_len != 1 || input.char_at(0) not in "<=>") {
        input = read_line(f"Is your number {guess}? [<, =, >]: ");
    }

    char input = input.char_at(0);
    if(input == '<') max = guess - 1;
    else if(input == '>') min = guess + 1;
    else {
        win = true;
        break;
    }
}

print(win ? "Nice" : "You cheated");